<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="laptop" class="org.practice.spring.SpringDemo.Laptop" primary="true"/>
	<bean id="desktop" class="org.practice.spring.SpringDemo.Desktop" />
	<bean id="person" class="org.practice.spring.SpringDemo.Person" autowire="byType">
		<property name="age" value="10" />
	</bean>

</beans>